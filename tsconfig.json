{
  "compilerOptions": {
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es2019",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "rootDirs": [
      "src"
    ],
    "baseUrl": "src",
  },
  "include": [
    "src"
  ],
  "exclude": []
}