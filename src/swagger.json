    }
  ],
  "host": "localhost:5000",
  "basePath": "/",
  "schemes": [
    "http",
    "https"
  ],
  "externalDocs": {
    "description": "SimpleQuiz API",
    "url": "https://github.com/edmilson-torres/simplequiz-api"
  },
  "paths": {
    "/api/quizzes": {
      "get": {
        "summary": "Quiz list",
        "tags": [
          "Quiz"
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "429": {
            "description": "Too many request"
          }
        }
      }
    },
    "/api/quiz/{id}": {
      "get": {
        "summary": "Get Quiz from Id",
        "tags": [
          "Quiz"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "description": "Quiz Id",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "429": {
            "description": "Too many request"
          }
        }
      }
    }
  }
}